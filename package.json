    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.0",
    "knex": "^0.21.13",
    "morgan": "^1.9.1",
    "pg": "^7.18.2",
    "postgrator-cli": "^3.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
